@model IEnumerable<Front_to_back.DataLAccesLayer.Entities.Student>

    <div class="row">
    <h2>Students</h2>

    </div>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Age</td>
            </tr>
        }
    </tbody>
</table>